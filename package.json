{
  "name": "handover-lib",
  "version": "0.0.3",
  "codename": "handy",
  "private": false,
  "type": "module",  
  "description": "A handover lib for balena",
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && depcheck --ignore-bin-package --ignores=@types/jest,typedoc",
    "lint:fix": "balena-lint --fix lib test",
    "test": "catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run test:integration",
    "doc": "typedoc lib/",
    "prepack": "npm run build"
  },
  "dependencies": {
    "@balena/jellyfish-environment": "^12.1.0",
    "@balena/jellyfish-logger": "^5.1.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "catch-uncommitted": "^2.0.0",
    "depcheck": "^1.4.3",
    "jest": "^27.5.1",
    "nock": "^13.2.6",
    "nodemon": "^2.0.16",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.8.1",
    "typedoc": "^0.23.5",
    "typescript": "^4.7.3"
  }
}
